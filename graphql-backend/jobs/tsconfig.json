{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "esModuleInterop": true,
    "outDir": "../dist",
    "rootDir": "..",
    "sourceMap": true,
    "strict": false,
    "skipLibCheck": true
  },
  "include": [
    "../src/**/*.ts",
    "../src/**/*.d.ts",
    "./**/*.ts"
  ],
  "exclude": [
    "../node_modules"
  ]
}
